{
  "name": "run-the-world",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://shiminshen.github.io/run-the-world",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.1",
    "@testing-library/user-event": "^12.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.0",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "_postinstall": "husky install",
    "prepublish": "pinst --disable",
    "postpublish": "pinst --enable",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "lintfix": "prettier --write src/**/*.{js,css} && eslint --fix",
    "lint": "prettier --write src/**/*.{js,css} && eslint",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "gh-pages": "^3.1.0",
    "husky": "^5.0.0-beta.0",
    "pinst": "^2.0.0",
    "prettier": "^2.1.2"
  }
}
